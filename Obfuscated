    loadstring(game:HttpGet("https://pastebin.com/raw/5WZCeftU"))()
elseif _G.SelectedSkinChange == "HatsPet(R15)" then
    loadstring(game:HttpGet("https://pastebin.com/raw/VD5LSKVn"))()
elseif _G.SelectedSkinChange == "HatsSpin(R15)" then
    local obese = game:GetService('Players')
    for i,v in pairs(obese.LocalPlayer.Character:GetChildren()) do
        if v.ClassName == "Accessory" then
            local stg = v.Handle:FindFirstChildOfClass("BodyForce")
                if stg == nil then
                    local a = Instance.new("BodyPosition")
                    local b = Instance.new("BodyAngularVelocity")
                    a.Parent = v.Handle
                    b.Parent = v.Handle
                    v.Handle.AccessoryWeld:Destroy()
                    b.AngularVelocity = Vector3.new(0,100,0)
                    b.MaxTorque = Vector3.new(0,200,0)
                    a.P = 30000
                    a.D = 50
                    game:GetService('RunService').Stepped:connect(function()
                    a.Position = obese.LocalPlayer.Character.Head.Position
                end)
            end
        end
    end
elseif _G.SelectedSkinChange == "BallRoll(R15)" then
    loadstring(game:HttpGet("https://pastebin.com/raw/TajXJjSx"))()
elseif _G.SelectedSkinChange == "Walkable Hat" then
    for _, v in pairs(game.Players.LocalPlayer.Character:getChildren()) do
	if v.ClassName == "Accessory" then
		for i, k in pairs(v:GetDescendants()) do
			if k.ClassName == "Attachment" then
				s = Instance.new("RopeConstraint")
				k.Parent.CanCollide = true
				s.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
				s.Attachment1 = k
				s.Attachment0 = game.Players.LocalPlayer.Character.Head.FaceCenterAttachment
				s.Visible = true
				s.Length = 10
				v.Handle.AccessoryWeld:Destroy()
			end
		end
	end
end
elseif _G.SelectedSkinChange == "Universal Hat(R6)" then
    loadstring(game:HttpGet("https://pastebin.com/raw/4B4fktPS", true))()
elseif _G.SelectedSkinChange == "Snake(R15/R6)" then
    loadstring(game:HttpGet(('https://pastefy.ga/tWBTcE4R/raw'),true))()
elseif _G.SelectedSkinChange == "GetTall(R15)" then
    venyx:Notify("GetTall(R15)", "Scale - Height 105%, Width 100%, Head 100%, Proportions 0% ")
    loadstring(game:HttpGet(('https://pastebin.com/raw/0KRjvLb9'),true))()
elseif _G.SelectedSkinChange == "GetSmall(R15)" then
    venyx:Notify("GetSmall(R15)", "Scale - Height 90%, Width 70%, Head 95%, Proportions 0% ")
    loadstring(game:HttpGet(('https://pastebin.com/raw/zf42ED7Q'),true))()
elseif _G.SelectedSkinChange == "Neko Anims(R15)" then
    loadstring(game:HttpGet(('https://pastebin.com/raw/r0UBKWws'),true))()
elseif _G.SelectedSkinChange == "HeadLess(R15)" then
    venyx:Notify("HeadLess(R15)", "Remove all your Hat")
    loadstring(game:HttpGet(('https://pastebin.com/raw/xjumaZB7'),true))()
elseif _G.SelectedSkinChange == "GiantHead(R15)" then
