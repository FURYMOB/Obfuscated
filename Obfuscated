-- üìá LuckyBlock üìá --
CompatibleSpyHub:addButton(game:GetService("MarketplaceService"):GetProductInfo(662417684).Name, function()
    game:GetService('TeleportService'):Teleport(662417684)
end)
if game.PlaceId == 662417684 then
    local LuckyBlockGet = CurentGame:addSection("LuckyBlock")
    LuckyBlockGet:addDropdown("Select Type", {"LuckyBlock", "SuperBlock", "DiamondBlock", "RainbowBlock", "GalaxyBlock"}, function(LuckyBlockGet)
        _G.LuckyBlockGet = LuckyBlockGet
    end)
    LuckyBlockGet:addSlider("Select How Many", 1, 1, 500, function(HowManyLucky)
        _G.HowManyLucky = HowManyLucky
    end)
    LuckyBlockGet:addButton("Execute", function()
        for count = 1, _G.HowManyLucky do
            if _G.LuckyBlockGet == "LuckyBlock" then
                game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
            elseif _G.LuckyBlockGet == "SuperBlock" then
                game:GetService("ReplicatedStorage").SpawnSuperBlock:FireServer()
            elseif _G.LuckyBlockGet == "DiamondBlock" then
                game:GetService("ReplicatedStorage").SpawnDiamondBlock:FireServer()
            elseif _G.LuckyBlockGet == "RainbowBlock" then
                game:GetService("ReplicatedStorage").SpawnRainbowBlock:FireServer()
            elseif _G.LuckyBlockGet == "GalaxyBlock" then
                game:GetService("ReplicatedStorage").SpawnGalaxyBlock:FireServer()
            end
        end
    end)
    LuckyBlockGet:addButton("Get All", function()
        for count = 1, 250 do
            game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
            game:GetService("ReplicatedStorage").SpawnSuperBlock:FireServer()
            game:GetService("ReplicatedStorage").SpawnDiamondBlock:FireServer()
            game:GetService("ReplicatedStorage").SpawnRainbowBlock:FireServer()
            game:GetService("ReplicatedStorage").SpawnGalaxyBlock:FireServer()
        end
    end)
end

