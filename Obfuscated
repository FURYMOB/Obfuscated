    local GorillaTag = CurentGame:addSection("Skin")
    GorillaTag:addToggle("RambowGorilla", false, function(RambowGorilla)
        _G.RambowGorilla = RambowGorilla
        while _G.RambowGorilla == true do
            wait()
            local args = {
                [1] = Color3.fromHSV(tick() % 20/20, 1, 1)
            }
    
            game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("TW's Game Gui").Side.Frames.SettingsFrame:FindFirstChild("2").Frame:FindFirstChild("TW's Script").TWsCRE:FireServer(unpack(args))
        end
    end)
    GorillaTag:addButton("MagicTools (Work 1 Time)", function()
        game:GetService("Workspace").Magic.Handle.Anchored = false
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Magic.Handle.CFrame
    end)
    GorillaTag:addButton("ToucheAll", function()
        player = game.Players.LocalPlayer
        for i,v in pairs(game.Players:GetChildren()) do
           if v.Name ~= player.Name then
               pcall(function()
                    for count = 1, 45 do
                        wait()
                        player.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                    end
               end)
           end
        end
    end)
end

