            if (v:IsA("Tool")) then
                v.Parent = game.Players.LocalPlayer.Backpack
            end
        end
    end)
    BuildABoatTools:addToggle("WaterGod", false, function(State)
        getgenv().Water = State 
        game:GetService('RunService').Stepped:connect(function()
            pcall(function()
                if getgenv().Water and game.Players.LocalPlayer.Character:FindFirstChild("WaterDetector")  then
                    game.Players.LocalPlayer.Character.WaterDetector:remove()
                end
            end)
        end)
    end)
    _G.Busy = false
    BuildABoatTools:addToggle("AutoFarm", false, function(AutoFarmBDB)
        _G.Busy = AutoFarmBDB
        if _G.Busy == true then
            loadstring(game:HttpGet("https://pastebin.com/raw/r1Ue47AV"))()
        end
    end)
end

